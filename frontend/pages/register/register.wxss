/* pages/register/register.wxss */
.head{
  position: fixed;
  width: 100%;
  height: 100rpx;
  margin-top:0;
  top:0;
}
.container{
  width:100%;
  position: absolute;
  margin-top:130rpx;
}
.regBlock {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
  /* border: 2rpx solid green; */
  height: 1800rpx;
  box-sizing: border-box;
  background: white;
}
.part {
  /* border: 1rpx solid red; */
  display: flex;
  width: 100%;
  /* margin-top: 20rpx; */
  height: 80rpx;
}
.instr{
  width: 20%;
  align-items: center;
  display: flex;
  justify-content: center;
}
.input {
  margin-left: 20rpx;
  /* border: 1rpx solid green; */
  height:100%;
  background: white;
  border-radius: 5rpx;
  width: 70%;
  box-sizing: border-box;
  border-left: 1rpx solid #dcdcdc;
  padding-left: 10rpx;
}
.captchaPart {
  /* border: 1rpx solid rebeccapurple; */
  display: flex;
  justify-content: space-evenly;
  height: 80rpx;
  width: 100%;
  /* margin-top: 20rpx; */
}
.inputCaptcha{
  /* border: 1rpx solid red; */
  margin-right: 5rpx;
  box-sizing: border-box;
  height: 100%;
  background: white;
  border-radius: 5rpx;
  padding-left: 10rpx;
}
.divLine{
  height: 1rpx;
  width: 100%;
  background: #dcdcdc;
  /* margin-top: 10rpx;
  margin-bottom: 10rpx; */
}
.registerPart{
  margin-top: 400rpx;
}
.regBtn{
  background: #4285f4;
  color: white;
}
.moneyPart{
  display: flex;
}
.moneyNote{
  width: 40%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.moneyBtn{
  display: flex;
  width: 60%;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.captchaBtn1{
  width: 300rpx!important;
  margin-top: 0!important;
  box-sizing: border-box!important;
  background: #4285f4!important;
  color: white!important;
}
.captchaBtn2{
  border: 1rpx solid #dcdcdc!important;
  margin-top: 0!important;
  box-sizing: border-box!important;
  width: 300rpx!important;
  color: black!important;
}
/* pages/register/register.wxss */
.head{
  position: fixed;
  width: 100%;
  height: 100rpx;
  margin-top:0;
  top:0;
}
.container{
  width:100%;
  position: absolute;
  margin-top:130rpx;
  background-color: white;
}
.regBlock {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
  /* border: 2rpx solid green; */
  box-sizing: border-box;
  background-color: white;
}
.part {
  display: flex;
  flex-direction: column;
  width: 100%;
}
.inputBlock{
  display: flex;
  justify-content: center;
}
.instr{
  font-size: medium;
  padding-left: 8%;
  margin-top: 50rpx;
  margin-bottom: 15rpx;
  color: #252525;
  /*
  width: 20%;
  align-items: center;
  display: flex;
  justify-content: center;
  */
}
.input {
  border: 1rpx solid rgb(212, 212, 212);
  background-color: white;
  box-shadow: 0px 2px 5px 5px rgb(240, 239, 239);
  height: 80rpx;
  width: 80%;
  border-radius: 50rpx;
  padding-left: 5%;
  font-size: medium;
  /*
  margin-left: 20rpx;
  height:100%;
  background: white;
  border-radius: 5rpx;
  width: 70%;
  box-sizing: border-box;
  border-left: 1rpx solid #dcdcdc;
  padding-left: 10rpx;
  */
}
.captchaPart {
  /* border: 1rpx solid rebeccapurple; */
  display: flex;
  justify-content: center;
  height: 80rpx;
  width: 100%;
}
.inputCaptcha{
  /* border: 1rpx solid red; */
  border: 1rpx solid rgb(212, 212, 212);
  background-color: white;
  box-shadow: 0px 2px 5px 5px rgb(240, 239, 239);
  height: 80rpx;
  width: 42%;
  border-radius: 50rpx;
  padding-left: 5%;
  font-size: medium;
}
.divLine{
  height: 1rpx;
  width: 100%;
  background: #dcdcdc;
  /* margin-top: 10rpx;
  margin-bottom: 10rpx; */
}
.registerPart{
  margin-top: 50rpx;
  width: 100%;
}
.regBtn{
  width: 100%;
}
.moneyPart{
  display: flex;
}
.moneyNote{
  width: 40%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.moneyBtn{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.captchaBtn1{
  width: 100%;
}
.captchaBtn2{
  width: 100%;
}
.errorNote{
  color:red;
  margin-left:100rpx;
  font-size: small;
  height: 30rpx;
}
.blank{
  height: 4vh;
}
.container {
  position: absolute;
  background-color: #f5f5f5;
  margin-top:130rpx;
}
.head{
  position: fixed;
  width:100%;
  height: 100rpx;
  margin-top:0;
}
.sectionImg {
  width: 100rpx;
  height: 100rpx;
}
.sectionName {
  /* border: 1rpx solid red; */
  text-align: center;
}
.swpItem{
  display: flex;
  justify-content: space-evenly;
}
.secUnit{
  /* border: 1rpx solid red; */
  height: 100rpx;
  margin-top: 20rpx;
}
.swpBlock{
  background: #ffffff;
  height: 250rpx;
  margin: 20rpx 30rpx 20rpx 30rpx;
  border-radius: 35rpx;
  border: 1rpx solid #dcdcdc;
}

.hotPart{
  border: 1rpx solid #dcdcdc;
  border-radius: 10rpx;
  margin: 5rpx 5rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items:stretch;
  background: white;
  border-radius: 5rpx;
}
.divLine{
  height: 1rpx;
  width: 100%;
  background: #dcdcdc;
  margin-top: 3rpx;
  /* margin-bottom: 3rpx; */
}
.top {
  width: 100%;
  height: 180px;
  background: #5484dd;
  padding-top: 15px;
  position: relative;
  margin-bottom: 30rpx;
}

.gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}
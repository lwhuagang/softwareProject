.searchBar{
  display: flex;
  justify-content: space-between;
  height: 50rpx;
  border: 1rpx solid red;
}
.searchBox {
  width: 80%;
  height: 100%;
  background: white;
  border: 1rpx solid red;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.sectionPart{
  white-space: nowrap;
  padding: 30rpx 0;
  border: 1rpx solid red;
}
.sectionPart .sectionItem {
  text-align: center;
  width: 165rpx;
  display: inline-block;
}
.sectionPart .sectionImg{
  width: 100rpx;
  height: 100rpx;
}
.sectionPart .sectionName{
  font-size: 26rpx;
  margin-top: 8rpx;
}
.searchBar{
  display: flex;
  justify-content: space-between;
  height: 50rpx;
  border: 1rpx solid red;
}
.searchBox {
  width: 80%;
  height: 100%;
  background: white;
  border: 1rpx solid red;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.sectionImg {
  width: 100rpx;
  height: 100rpx;
}
.sectionName {
  border: 1rpx solid red;
  text-align: center;
}
.swpItem{
  display: flex;
  justify-content: space-evenly;
}
.secUnit{
  border: 1rpx solid red;
  height: 100rpx;
  margin-top: 20rpx;
}
.swpBlock{
  height: 250rpx;
  border: 1rpx solid green;
  margin: 10rpx 10rpx;
  border-radius: 20rpx;
}
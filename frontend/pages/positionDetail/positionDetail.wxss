.container {
  height: 1500px;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.positionPie {
  width: 100%;
  height: 100%;
}
.positionPart{
  background: white;
  width: 100%;
  height: 400rpx;
  border: 1rpx solid #dcdcdc;
  border-radius: 5rpx;
  display: flex;
  flex-direction: column;
}
.positionNote{
  width: 98%;
  height: 80rpx;
  border-bottom: 1rpx solid #dcdcdc;
  font-size: large;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 10rpx;
}
.total{
  position: absolute;
  margin-top: 180rpx;
  margin-left:300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* border: 1rpx solid red; */
}
.totalNum{
  font-size: small;
}
.totalNote{
  font-size: smaller;
}
.totalDate{
  font-size: smaller;
}
.stockPart{
  width: 100%;
  border-top: 1rpx solid #f5f5f5;
  border-left: 1rpx solid #dcdcdc;
  border-right: 1rpx solid #dcdcdc;
  border-bottom: 1rpx solid #dcdcdc;
  border-bottom-left-radius: 5rpx;
  border-bottom-right-radius: 5rpx;
  display: flex;
  flex-direction: column;
  background: white;
}
.stockNote{
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-evenly;
  border-bottom: 2rpx solid #dcdcdc;
  box-sizing: border-box;
  color: #6b6b6b;
}
.stockNameNote{
  width: 25%;
  height: 100%;
  margin-left: 10rpx;
}
.stockPriceNote{
  width: 25%;
  height: 100%;
  text-align: center;
}
.stockPercentNote{
  width: 25%;
  height: 100%;
  text-align: center;
}
.stockNumNote{
  width: 25%;
  height: 100%;
  text-align: right;
}
.stockItem{
  display: flex;
  width: 100%;
  height: 90rpx;
  justify-content: space-evenly;
  border-bottom: 1rpx solid #dcdcdc;
  box-sizing: border-box;
}
.stockNameAndCode{
  width: 25%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}
.stockName{
  margin-left: 10rpx;
}
.stockCode{
  font-size: small;
  color: #c6c6c6;
  margin-left: 10rpx;
}
.stockPrice{
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.stockPercent{
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.stockNum{
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
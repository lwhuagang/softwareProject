.head{
  position: fixed;
  width: 100%;
  height: 100rpx;
  margin-top:0;
  top:0;
}
.container {
  height: 1500px;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  width: 100%;
  margin-top:130rpx;
}
.positionPie {
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  width: 100%;
  height: 100%;
}
.positionPart{
  background: white;
  width: 100%;
  height: 550rpx;
  border: 1rpx solid #ebebeb;
  border-radius: 15rpx;
  box-shadow:0px 2px 5px 5px rgb(240, 239, 239);
  display: flex;
  flex-direction: column;
  margin-top: 50rpx;
}
.positionNote{
  width: 100%;
  height: 130rpx;
  border-bottom: 1rpx solid #dcdcdc;
  font-size: medium;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 10rpx;
  padding-right: 10rpx;
}
.total{
  position: absolute;
  margin-top: 250rpx;
  margin-left:310rpx;
  /* 如果本模块前面样式修改了，这里很有可能对不到中心去 */
  display: flex;
  flex-direction: column;
  align-items: center;
  /* border: 1rpx solid red; */
}
.totalNum{
  font-size: small;
}
.totalNote{
  font-size: smaller;
}
.totalDate{
  font-size: smaller;
}
.stockPart{
  width: 100%;
  border-top: 1rpx solid #f5f5f5;
  border-left: 1rpx solid #dcdcdc;
  border-right: 1rpx solid #dcdcdc;
  border-bottom: 1rpx solid #dcdcdc;
  border-bottom-left-radius: 5rpx;
  border-bottom-right-radius: 5rpx;
  display: flex;
  flex-direction: column;
  background: white;
}
.stockNote{
  width: 100%;
  height: 70rpx;
  padding-top:8rpx;
  display: flex;
  justify-content: space-evenly;
  border-bottom: 2rpx solid #dcdcdc;
  box-sizing: border-box;
  color: #6b6b6b;
}
.stockNameNote{
  width: 25%;
  height: 100%;
  margin-left: 10rpx;
}
.stockPriceNote{
  width: 25%;
  height: 100%;
  text-align: center;
}
.stockPercentNote{
  width: 25%;
  height: 100%;
  text-align: center;
}
.stockNumNote{
  width: 25%;
  height: 100%;
  text-align: right;
}
.stockItem{
  display: flex;
  width: 100%;
  height: 100rpx;
  justify-content: space-evenly;
  border-bottom: 1rpx solid #dcdcdc;
  box-sizing: border-box;
}
.stockNameAndCode{
  width: 25%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}
.stockName{
  width: 100%;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
  margin-left: 10rpx;
}
.stockCode{
  font-size: small;
  color: #c6c6c6;
  margin-left: 10rpx;
}
.stockPrice{
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.stockPercent{
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.stockNum{
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
/* pages/rank/rank.wxss */
.head{
  position: fixed;
  width: 100%;
  height: 100rpx;
  margin-top:0;
  top:0;
}
.container{
  width:100%;
  position: absolute;
  margin-top:130rpx;
}
.listHeadItem {
  width: 350px;
  font-size: 16px;
  position: relative;
  height: 35px;
  line-height: 30px;
  background-color: #e6e6e6;
  align-items: center;
  border-radius: 20rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align-last: right;
  padding: 2px 7px 0 0px;
  font-size: 15px;
  color:rgb(93, 93, 95);
}

.growth{
  float: right;
}

.selectHead {
  background: #e6e6e6;
  display: flex;
  position: fixed;
  margin-top:130rpx;
  width:100%;
}

.searchResContainer {
  background: white;
}

.divLine{
  height: 1rpx;
  width: 100%;
  background: #dcdcdc;
  margin-top: 3rpx;
  margin-bottom: 3rpx;
}
.divLine1{
  height: 1rpx;
  width: 100%;
  background: #dcdcdc;
  margin-top: 3rpx;
  margin-bottom: 80rpx;
}
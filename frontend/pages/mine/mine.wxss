Page {
  font-size: 14px;
}
.head{
  position: fixed;
  width: 100%;
  height: 100rpx;
  margin-top:0;
}
.container{
  position: absolute;
  width: 100%;
  margin-top:130rpx;
}
.loginContainer {
  border: 1rpx solid green;
  display: flex;
  flex-direction: column;
  position: relative;
  align-items: center;
  height: 1500rpx;
  background: #f5f5f5;
}
.loginImg{
  position: absolute;
  width: 200rpx;
  margin-top: 50rpx;
  background: white;
  border-radius: 50%;
}
.loginInfo {
  display: flex;
  justify-content: center;
  position: absolute;
  height: 500rpx;
  width: 450rpx;
  border: 10rpx solid #1296db;
  border-radius: 20rpx;
  margin-top: 160rpx;
  background: white;
}
.inputUsrName {
  border: 1rpx solid gray;
  position: absolute;
  margin-top: 120rpx;
  height: 58rpx;
  width: 88%;
  border-radius: 10rpx;
  padding: 5rpx;
}
.inputUsrPassword {
  border: 1rpx solid gray;
  position: absolute;
  margin-top: 210rpx;
  height: 58rpx;
  width: 88%;
  border-radius: 10rpx;
  padding: 5rpx;
}
.loginBtn {
  position: absolute;
  margin-top:480rpx;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #1296db;
}
.registerBtn{
  border: 1rpx solid #1296db;
  position: absolute;
  margin-top: 570rpx;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.userEmailPart{
  display: flex;
  border: 1rpx solid red;
}

.top {
  width: 100%;
  height: 200px;
  background: #5484dd;
  padding-top: 15px;
  position: relative;
}

.center {
  width: 95%;
  height: 160px;
  background: white;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  border-radius: 5px;
}

.center_top {
  display: flex;
  flex-direction: row;
  width: 80%;
  height: 80px;
  margin: 0 auto;
  margin-top: 20rpx;
  border-bottom: 1px solid #EEEEEE;
}

.center_img {
  width: 66px;
  height: 66px;
  border-radius: 50%;
  overflow: hidden;
}

.center_img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.center_img .user_head {
  width: 100%;
  height: 100%;
}

.center_info {
  display: flex;
  flex-direction: column;
  margin-top: 10rpx;
  margin-left: 30px;
}

.center_name {
  font-size: 20px;
}

.center_email {
  color: #BEBEBE;
}

.center_down {
  display: flex;
  flex-direction: row;
  width: 80%;
  height: 35px;
  margin: 0 auto;
  margin-top: 20rpx;
}

.center_rank {
  width: 50%;
  height: 35px;
  display: flex;
  flex-direction: row;
}

.rank_text {
  height: 35px;
  line-height: 35px;
  margin-left: 10rpx;
  color: #AAAAAA;
}

.center_rank image {
  width: 35px;
  height: 35px;
}

.center_score {
  width: 50%;
  height: 35px;
  display: flex;
  flex-direction: row;
}

.center_score image {
  width: 35px;
  height: 35px;
}

.gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}
.logOutBtn{
  margin-top:500rpx;
}
.feedback{
  text-align: center;
  background: white;
  font-size: large;
  color: black;
}
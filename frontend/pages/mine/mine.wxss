.loginContainer {
  border: 1rpx solid green;
  display: flex;
  flex-direction: column;
  position: relative;
  align-items: center;
  height: 2000rpx;
  background: #f5f5f5;
}
.loginImg{
  position: absolute;
  width: 200rpx;
  margin-top: 50rpx;
  background: white;
  border-radius: 50%;
}
.loginInfo {
  display: flex;
  justify-content: center;
  position: absolute;
  height: 500rpx;
  width: 450rpx;
  border: 10rpx solid #1296db;
  border-radius: 20rpx;
  margin-top: 160rpx;
  background: white;
}
.inputUsrName {
  border: 1rpx solid gray;
  position: absolute;
  margin-top: 120rpx;
  height: 58rpx;
  width: 88%;
  border-radius: 10rpx;
  padding: 5rpx;
}
.inputUsrPassword {
  border: 1rpx solid gray;
  position: absolute;
  margin-top: 210rpx;
  height: 58rpx;
  width: 88%;
  border-radius: 10rpx;
  padding: 5rpx;
}
.loginBtn {
  position: absolute;
  margin-top:480rpx;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #1296db;
}
.registerBtn{
  border: 1rpx solid #1296db;
  position: absolute;
  margin-top: 570rpx;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.userEmailPart{
  display: flex;
  border: 1rpx solid red;
}
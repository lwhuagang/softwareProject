.head {
  position: fixed;
  width: 100%;
  height: 100rpx;
  margin-top: 0;
}

.container {
  position: absolute;
  width: 100%;
  margin-top: 130rpx;

}

.record {
  display: flex;
  /* border: 1px solid red; */
  flex-wrap: nowrap;
  background-color: white;
  height: 155rpx;
}

.state {
  border-radius: 10rpx;
  background-color: #eaf0ff;
  width: 68px;
}

.typeName {
  /* border: 1px solid green; */
  color: #3567b3;
  background-color: #eaf0ff;
  border-radius: 15rpx;
  font-size: 40rpx;
  margin-left: 1px;
  padding-left: 12px;
  padding-top: 15px;
  width: 50px;
}

.ing {
  /* border: 1px solid green; */
  color: #3567b3;
  background-color: #eaf0ff;
  border-radius: 15rpx;
  font-size: 24rpx;
  margin-left: 1px;
  padding-left: 14px;
  padding-top: 3px;
}

.finish {
  /* border: 1px solid green; */
  color: #53575e;
  background-color: #eaf0ff;
  border-radius: 15rpx;
  font-size: 24rpx;
  margin-left: 1px;
  padding-left: 10px;
  padding-top: 3px;
}

.fundNameAndTime {
  width: 400rpx;
  padding-left: 20px;
  padding-top: 20px;
  /* border: 1px solid purple; */
  font-size: 30rpx;
  color: #555454;
}

.fundNameAndTime .tradeTime {
  font-size: 13px;
  color: #777777;
}

.MoneyAndDelete {
  padding-left: 10px;
  float: right;
}

.tradeNum {
  /* border: 1px solid blue; */
  padding-top: 17px;
  padding-bottom: 10rpx;
  color: #555454;
  overflow-wrap: break-word;
  font-size: 30rpx;
}

.delete {
  width: 70px;
  padding-top: 4rpx;
  padding-bottom: 4rpx;
  border: solid #11244b;
  border-width: 1rpx;
  background-color: #eaf0ff;
  color: #252b33;
  text-align: center;
  border-radius: 20px;
  float: right;
  font-size: 14px;
}

.divLine {
  width: 100%;
  background: #9e9d9d;
  height: 3rpx;
}
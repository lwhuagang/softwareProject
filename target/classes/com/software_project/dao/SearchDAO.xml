<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.software_project.dao.SearchDAO">
    <insert id="insertSearch" parameterType="com.software_project.pojo.Search">
        insert into soft.searchhistory values (#{userEmail}, #{searchString});
    </insert>
    <delete id="deleteSearch" parameterType="java.lang.String">
        delete from soft.searchhistory where userEmail = #{userEmail}
    </delete>
    <delete id="deleteOneSearch" parameterType="com.software_project.pojo.Search">
        delete from soft.searchhistory where userEmail = #{userEmail} and searchString = #{searchString}
    </delete>
    <select id="findSearchByEmail" resultType="com.software_project.pojo.Search"
            parameterType="java.lang.String">
        select * from soft.searchhistory where userEmail = #{userEmail};
    </select>

</mapper>